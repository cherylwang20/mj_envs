<mujoco model="benchmark_case">
    <compiler inertiafromgeom='auto' angle='radian' eulerseq='xyz'/>
    <size njmax="5000" nconmax="1000" />
    <option timestep='0.001' iterations='50' solver='Newton' gravity='0 0 -9.81' cone="elliptic" tolerance="1e-10" impratio="10"/>
    <include file="../../../simhive/scene_sim/topfloor_scene.xml"/>
    <include file="../../../simhive/furniture_sim/simpleTable/simpleTable_asset.xml"/>
    <include file="../../../simhive/furniture_sim/ventionTable/ventionTable_asset.xml"/>
    <include file="../../../simhive/furniture_sim/bin/bin_asset.xml"/>

    <default>
        <default class="ur10e_actuator">
            <position ctrllimited="true" ctrlrange="-6.2831 6.2831" kp="16000" forcelimited="true"></position >
            <material specular="0.5" shininess="0.25"/>
            <joint axis="0 1 0" limited = 'true' range="-6.28319 6.28319" armature="0.1"/>
            <general biastype="affine" ctrlrange="-6.2831 6.2831" gainprm="5000" biasprm="0 -5000 -500"/>
            <default class="size4">
                <joint damping="10"/>
                <general forcerange="-330 330"/>
            </default>
            <default class="size3">
                <joint damping="5"/>
                <general forcerange="-150 150"/>
                <default class="size3_limited">
                <joint range="-3.1415 3.1415"/>
                <general ctrlrange="-3.1415 3.1415"/>
                </default>
            </default>
            <default class="size2">
                <joint damping="2"/>
                <general forcerange="-56 56"/>
            </default>
            <default class="visual">
                <geom type="mesh" contype="0" conaffinity="0" group="2"/>
            </default>
            <default class="collision">
                <geom type="capsule" group="3"/>
                <default class="eef_collision">
                <geom type="cylinder"/>
                </default>
            </default>
            <site size="0.001" rgba="0.5 0.5 0.5 0.3" group="4"/>
            
        </default>


        <default class="3fGripper">
            <material specular="0.5" shininess="0.25"/>
            <position ctrllimited="true" kp="80" forcelimited="true"></position >
        </default>
    </default>
    
    <asset>
        <include file="include/ur10e_3fGripper_assets.xml"/>


        <texture type="skybox" builtin="gradient" rgb1="0.3 0.5 0.7" rgb2="0 0 0" width="512" height="512"/>
        <material class="ur10e_actuator" name="black" rgba="0.033 0.033 0.033 1"/>
        <material class="ur10e_actuator" name="jointgray" rgba="0.278 0.278 0.278 1"/>
        <material class="ur10e_actuator" name="linkgray" rgba="0.82 0.82 0.82 1"/>
        <material class="ur10e_actuator" name="urblue" rgba="0.49 0.678 0.8 1"/>
    </asset>



    <worldbody>
        <light directional='true' diffuse='0.5 0.5 0.5' specular='0.3 0.3 0.3' pos='0 0 0' dir='0 -.5 -1'/>
        <camera name='right_cam' pos="-3.415 2.732 2.719" xyaxes="-0.655 -0.756 0.000 0.321 -0.278 0.905"/>

        <!-- Roboter -->
        <include file="urdfs/URDF_conv/ur10e.xml"/>

        <body name="ventiontable" pos='0 -.3 0'>
            <include file="../../../simhive/furniture_sim/ventionTable/ventionTable_body.xml"/>
        </body>
        <body name="scenetable" pos='0 0.49 0.065'>
            <include file="../../../simhive/furniture_sim/simpleTable/simpleMarbleTable_body.xml"/>
        </body>

        <body name="obj1" pos="0.253 0.675 1">
            <inertial pos="0 0 0" mass="0.055" diaginertia="0.0001 0.0001 0.0001" />
            <geom type="sphere" size="0.03" pos="0.06 0.05 0.005" euler="1.2 .5 .9" group="1" rgba=".31 .37 .7 1" mass=".045"/>
            <geom type="cylinder" size="0.026 0.06" pos="0.03 0.012 .01" euler=".1 .6 .2" group="1" rgba=".6 .3 .8 1" mass=".010"/>
            <geom type="box" size="0.045 0.023 0.032" pos="0.02 0.012 .02" euler=".4 .5 .4" group="1" rgba=".12 .5 .2 1" mass=".004"/>
            <site name="obj1" size="0.01" pos="0 0 0"/>
            <freejoint/>
        </body>

        <site name='drop_target' type='cylinder' size='.088 0.021' pos='-.235 0.5 .85' group='1' rgba='.1 .8 .2 .4'/>

    </worldbody>

    <actuator>
        <position  class="ur10e_actuator" joint="shoulder_pan_joint" name="shoulder_pan_act" ctrlrange="-6.28318530718 6.28318530718" forcerange="-330 330" kp="10000"></position >
        <position  class="ur10e_actuator" joint="shoulder_lift_joint" name="shoulder_lift_act" ctrlrange="-6.28318530718 6.28318530718" forcerange="-330 330" kp="25000"></position >
        <position  class="ur10e_actuator" joint="elbow_joint" name="elbow_act" ctrlrange="-6.28318530718 6.28318530718" forcerange="-150 150" kp="25000"></position >
        <position  class="ur10e_actuator" joint="wrist_1_joint" name="wrist_1_act" ctrlrange="-6.28318530718 6.28318530718" forcerange="-56 56" kp="15000"></position >
        <position  class="ur10e_actuator" joint="wrist_2_joint" name="wrist_2_act" ctrlrange="-6.28318530718 6.28318530718" forcerange="-56 56" kp="1800"></position >
        <position  class="ur10e_actuator" joint="wrist_3_joint" name="wrist_3_act" ctrlrange="-6.28318530718 6.28318530718" forcerange="-56 56" kp="1800"></position >

        <position  class="3fGripper" joint="palm_finger_1_joint" name="palm_finger_1_act" ctrlrange="-0.1784 0.192" forcerange="-100 100"></position >
        <position  class="3fGripper" joint="finger_1_joint_1" name="finger_1_joint_1_act" ctrlrange="0.0495 1.2218" forcerange="-4 4" kp="20"></position >
        <position  class="3fGripper" joint="finger_1_joint_2" name="finger_1_joint_2_act" ctrlrange="0 1.5708" forcerange="-100 100"></position >
        <position  class="3fGripper" joint="finger_1_joint_3" name="finger_1_joint_3_act" ctrlrange="-1.2217 -0.0523" forcerange="-4 4"></position >
        <position  class="3fGripper" joint="palm_finger_2_joint" name="palm_finger_2_act" ctrlrange="-0.192 0.1784" forcerange="-100 100"></position >
        <position  class="3fGripper" joint="finger_2_joint_1" name="finger_2_joint_1_act" ctrlrange="0.0495 1.2218" forcerange="-4 4" kp="20"></position >
        <position  class="3fGripper" joint="finger_2_joint_2" name="finger_2_joint_2_act" ctrlrange="0 1.5708" forcerange="-100 100"></position >
        <position  class="3fGripper" joint="finger_2_joint_3" name="finger_2_joint_3_act" ctrlrange="-1.2217 -0.0523" forcerange="-4 4"></position >
        <position  class="3fGripper" joint="finger_middle_joint_1" name="finger_middle_joint_1_act" ctrlrange="0.0495 1.2218" forcerange="-8 8"></position >
        <position  class="3fGripper" joint="finger_middle_joint_2" name="finger_middle_joint_2_act" ctrlrange="0 1.5708" forcerange="-100 100"></position >
        <position  class="3fGripper" joint="finger_middle_joint_3" name="finger_middle_joint_3_act" ctrlrange="-1.2217 -0.0523" forcerange="-8 8"></position >

    </actuator>

    
    <sensor>
        <jointpos name="ur_arm_jp1" joint="shoulder_pan_joint"/>
        <jointpos name="ur_arm_jp2" joint="shoulder_lift_joint"/>
        <jointpos name="ur_arm_jp3" joint="elbow_joint"/>
        <jointpos name="ur_arm_jp4" joint="wrist_1_joint"/>
        <jointpos name="ur_arm_jp5" joint="wrist_2_joint"/>
        <jointpos name="ur_arm_jp6" joint="wrist_3_joint"/>
        <jointvel name="ur_arm_jv1" joint="shoulder_pan_joint"/>
        <jointvel name="ur_arm_jv2" joint="shoulder_lift_joint"/>
        <jointvel name="ur_arm_jv3" joint="elbow_joint"/>
        <jointvel name="ur_arm_jv4" joint="wrist_1_joint"/>
        <jointvel name="ur_arm_jv5" joint="wrist_2_joint"/>
        <jointvel name="ur_arm_jv6" joint="wrist_3_joint"/>
    </sensor>

    <keyframe>
        <key qpos='-0.13 -0.63 -0.35 -0.69 -1.5 0.00014 0.0023 0.049 -4.1e-08 -0.052 0.0023 0.049 -4.1e-08 -0.052 0.05 1.5e-06 -0.052 0.22 0.65 0.85 0.81 0.57 -0.1 -0.051'/>
    </keyframe>

</mujoco>
